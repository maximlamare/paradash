{
  "name": "paradash2",
  "version": "1.0.0",
  "description": "Paragliding flight tracker application",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "test": "vitest",
    "test:coverage": "vitest run --coverage",
    "server": "node server.js",
    "start": "concurrently \"npm run server\" \"npm run dev\"",
