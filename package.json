    "@capacitor/filesystem": "^7.1.5",
    "@capacitor/share": "^7.0.2",
    "adm-zip": "^0.5.10",
    "archiver": "^7.0.0",
    "cors": "^2.8.5",
    "express": "^4.18.0",
    "igc-parser": "^2.0.0",
    "leaflet": "^1.9.4",
    "leaflet.fullscreen": "^5.1.1",
    "multer": "^2.0.2",
    "sqlite3": "^5.1.0",
    "vue": "^3.4.0",
    "vue-router": "^4.2.0"
  },
  "devDependencies": {
    "@capacitor/assets": "^3.0.5",
    "@capacitor/cli": "^7.4.4",
    "@vitejs/plugin-vue": "^5.0.0",
    "@vue/test-utils": "^2.4.0",
    "concurrently": "^8.2.0",
